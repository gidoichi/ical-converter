{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "branchConcurrentLimit": 0,
  "labels": ["dependencies"],
  "schedule": "* * 1 * *",
  "packageRules": [
    {
      "groupName": "dependencies",
      "matchPackagePatterns": [".*"],
      "automerge": true,
      "commitMessagePrefix": "[automerged]",
      "commitMessageExtra": null
    }
  ]
}
